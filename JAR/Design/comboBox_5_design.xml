<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent">
	<include
	layout="@layout/comboBox_5_guideline">
	</include>
	<Button
	android:id="@+id/button_1"
	android:layout_width="0dp"
	android:layout_height="0dp"
	android:text="ClickMe"
	app:layout_constraintStart_toEndOf="@+id/button_1_start_guideline"
	app:layout_constraintEnd_toStartOf="@+id/button_1_end_guideline"
	app:layout_constraintTop_toBottomOf="@+id/button_1_top_guideline"
	app:layout_constraintBottom_toTopOf="@+id/button_1_bottom_guideline">
	</Button>
	<CheckBox
	android:id="@+id/checkBox_off_1"
	android:layout_width="0dp"
	android:layout_height="0dp"
	android:checked="false"
	android:text="null"
	app:layout_constraintStart_toEndOf="@+id/checkBox_off_1_start_guideline"
	app:layout_constraintEnd_toStartOf="@+id/checkBox_off_1_end_guideline"
	app:layout_constraintTop_toBottomOf="@+id/checkBox_off_1_top_guideline"
	app:layout_constraintBottom_toTopOf="@+id/checkBox_off_1_bottom_guideline">
	</CheckBox>
	<CheckBox
	android:id="@+id/checkBox_on_2"
	android:layout_width="0dp"
	android:layout_height="0dp"
	android:checked="true"
	android:text="null"
	app:layout_constraintStart_toEndOf="@+id/checkBox_on_2_start_guideline"
	app:layout_constraintEnd_toStartOf="@+id/checkBox_on_2_end_guideline"
	app:layout_constraintTop_toBottomOf="@+id/checkBox_on_2_top_guideline"
	app:layout_constraintBottom_toTopOf="@+id/checkBox_on_2_bottom_guideline">
	</CheckBox>
	<Spinner
	android:id="@+id/comboBox_1"
	android:layout_width="0dp"
	android:layout_height="0dp"
	app:layout_constraintStart_toEndOf="@+id/comboBox_1_start_guideline"
	app:layout_constraintEnd_toStartOf="@+id/comboBox_1_end_guideline"
	app:layout_constraintTop_toBottomOf="@+id/comboBox_1_top_guideline"
	app:layout_constraintBottom_toTopOf="@+id/comboBox_1_bottom_guideline">
	</Spinner>
	<EditText
	android:id="@+id/editText_1"
	android:layout_width="0dp"
	android:layout_height="0dp"
	app:layout_constraintStart_toEndOf="@+id/editText_1_start_guideline"
	app:layout_constraintEnd_toStartOf="@+id/editText_1_end_guideline"
	app:layout_constraintTop_toBottomOf="@+id/editText_1_top_guideline"
	app:layout_constraintBottom_toTopOf="@+id/editText_1_bottom_guideline">
	</EditText>
	<ImageView
	android:id="@+id/imageView_1"
	android:layout_width="0dp"
	android:layout_height="0dp"
	app:layout_constraintStart_toEndOf="@+id/imageView_1_start_guideline"
	app:layout_constraintEnd_toStartOf="@+id/imageView_1_end_guideline"
	app:layout_constraintTop_toBottomOf="@+id/imageView_1_top_guideline"
	app:layout_constraintBottom_toTopOf="@+id/imageView_1_bottom_guideline">
	</ImageView>
	<ListView
	android:id="@+id/listView_1"
	android:layout_width="0dp"
	android:layout_height="0dp"
	app:layout_constraintStart_toEndOf="@+id/listView_1_start_guideline"
	app:layout_constraintEnd_toStartOf="@+id/listView_1_end_guideline"
	app:layout_constraintTop_toBottomOf="@+id/listView_1_top_guideline"
	app:layout_constraintBottom_toTopOf="@+id/listView_1_bottom_guideline">
	</ListView>
	<ProgressBar
	android:id="@+id/progressBar_1"
	android:layout_width="0dp"
	android:layout_height="0dp"
	app:layout_constraintStart_toEndOf="@+id/progressBar_1_start_guideline"
	app:layout_constraintEnd_toStartOf="@+id/progressBar_1_end_guideline"
	app:layout_constraintTop_toBottomOf="@+id/progressBar_1_top_guideline"
	app:layout_constraintBottom_toTopOf="@+id/progressBar_1_bottom_guideline">
	</ProgressBar>
	<RadioButton
	android:id="@+id/radioButton_off_1"
	android:layout_width="0dp"
	android:layout_height="0dp"
	android:checked="false"
	android:text="null"
	app:layout_constraintStart_toEndOf="@+id/radioButton_off_1_start_guideline"
	app:layout_constraintEnd_toStartOf="@+id/radioButton_off_1_end_guideline"
	app:layout_constraintTop_toBottomOf="@+id/radioButton_off_1_top_guideline"
	app:layout_constraintBottom_toTopOf="@+id/radioButton_off_1_bottom_guideline">
	</RadioButton>
	<RadioButton
	android:id="@+id/radioButton_on_2"
	android:layout_width="0dp"
	android:layout_height="0dp"
	android:checked="true"
	android:text="null"
	app:layout_constraintStart_toEndOf="@+id/radioButton_on_2_start_guideline"
	app:layout_constraintEnd_toStartOf="@+id/radioButton_on_2_end_guideline"
	app:layout_constraintTop_toBottomOf="@+id/radioButton_on_2_top_guideline"
	app:layout_constraintBottom_toTopOf="@+id/radioButton_on_2_bottom_guideline">
	</RadioButton>
	<SeekBar
	android:id="@+id/slidBar_1"
	android:layout_width="0dp"
	android:layout_height="0dp"
	app:layout_constraintStart_toEndOf="@+id/slidBar_1_start_guideline"
	app:layout_constraintEnd_toStartOf="@+id/slidBar_1_end_guideline"
	app:layout_constraintTop_toBottomOf="@+id/slidBar_1_top_guideline"
	app:layout_constraintBottom_toTopOf="@+id/slidBar_1_bottom_guideline">
	</SeekBar>
	<Switch
	android:id="@+id/switch_1"
	android:layout_width="0dp"
	android:layout_height="0dp"
	android:checked="false"
	app:layout_constraintStart_toEndOf="@+id/switch_1_start_guideline"
	app:layout_constraintEnd_toStartOf="@+id/switch_1_end_guideline"
	app:layout_constraintTop_toBottomOf="@+id/switch_1_top_guideline"
	app:layout_constraintBottom_toTopOf="@+id/switch_1_bottom_guideline">
	</Switch>
	<TextView
	android:id="@+id/textView_1"
	android:layout_width="0dp"
	android:layout_height="0dp"
	android:text="HelloXML"
	app:layout_constraintStart_toEndOf="@+id/textView_1_start_guideline"
	app:layout_constraintEnd_toStartOf="@+id/textView_1_end_guideline"
	app:layout_constraintTop_toBottomOf="@+id/textView_1_top_guideline"
	app:layout_constraintBottom_toTopOf="@+id/textView_1_bottom_guideline">
	</TextView>
</android.support.constraint.ConstraintLayout>
